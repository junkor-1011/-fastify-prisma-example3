{
  "name": "fastify-prisma-example",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "packageManager": "pnpm@7.18.0",
  "scripts": {
    "build": "esbuild src/app.ts --outdir=dist --platform=node --minify --bundle --tsconfig=tsconfig-prod.json",
    "dev:tsnd": "tsnd --respawn --transpile-only --exit-child -r tsconfig-paths/register --project tsconfig-dev.json src/app.ts",
    "dev": "tsx watch src/app.ts",
    "start": "node dist/app.js",
    "unit": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "test": "pnpm unit",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --fix --ext .ts src/",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "keywords": [],
  "private": true,
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^9.2.2",
    "@types/node": "^18.11.10",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "esbuild": "^0.15.17",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-define-config": "^1.12.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "prisma": "^4.7.1",
    "prisma-erd-generator": "^1.2.4",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.1",
    "tsx": "^3.12.1",
    "typescript": "~4.9.3"
  },
  "dependencies": {
    "@fastify/swagger": "^8.2.0",
    "@fastify/swagger-ui": "^1.3.0",
    "@prisma/client": "^4.7.1",
    "fastify": "^4.10.2",
    "fastify-zod": "^1.2.0",
    "uuid": "^9.0.0",
    "zod": "^3.19.1"
  }
}
