{
  "name": "fastify-prisma-example",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "main": "index.js",
  "packageManager": "pnpm@8.3.1",
  "scripts": {
    "build:legacy": "esbuild src/main.ts --outdir=dist --platform=node --minify --bundle --tsconfig=tsconfig-prod.json --format=esm --out-extension:.js=.mjs --banner:js='import { createRequire as topLevelCreateRequire } from \"module\"; import url from \"url\"; const require = topLevelCreateRequire(import.meta.url); const __filename = url.fileURLToPath(import.meta.url); const __dirname = url.fileURLToPath(new URL(\".\", import.meta.url));'",
    "build": "tsx scripts/build.ts",
    "dev": "tsx watch src/main.ts",
    "start": "dotenv -e .env.production.default -e .env.production node dist/main.mjs",
    "generate": "prisma generate",
    "cleanup": "tsx prisma/cleanup.ts",
    "seed": "tsx prisma/seed.ts",
    "studio": "prisma studio --browser none",
    "unit": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "test": "NODE_OPTIONS='--experimental-vm-modules' pnpm unit",
    "test:integration": "NODE_OPTIONS='--experimental-vm-modules' jest -c jest.config-integration.cjs --runInBand",
    "test:ts-jest": "NODE_OPTIONS='--experimental-vm-modules' jest -c jest.config-tsjest.cjs --passWithNoTests",
    "test:integration:ts-jest": "NODE_OPTIONS='--experimental-vm-modules' TEST_TARGET='integration' jest -c jest.config-tsjest.cjs --runInBand --passWithNoTests",
    "lint": "eslint --ext .ts src/ test/ prisma/",
    "lint:fix": "eslint --fix --ext .ts src/ test/ prisma/",
    "format": "prettier --write src test prisma ./*.{js,json,yaml,yml,md}",
    "dependency-check": "dependency-cruise src",
    "dependency-graph": "scripts/gen_depgraph",
    "docs:openapi": "tsx scripts/genOpenapiYaml.ts",
    "prepare": "husky install"
  },
  "keywords": [],
  "private": true,
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@mermaid-js/mermaid-cli": "^10.1.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "dependency-cruiser": "^12.11.3",
    "dotenv-cli": "^7.2.1",
    "esbuild": "^0.17.18",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-define-config": "^1.20.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "lint-staged": "^13.2.2",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.8",
    "prisma": "^4.13.0",
    "prisma-erd-generator": "^1.5.4",
    "ts-jest": "^29.1.0",
    "ts-jest-resolver": "^2.0.1",
    "tsx": "^3.12.6",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "@fastify/sensible": "^5.2.0",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.8.0",
    "@prisma/client": "^4.13.0",
    "fastify": "^4.17.0",
    "fastify-zod": "^1.3.2",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "pnpm": {
    "overrides": {
      "@nestjs/core@<9.0.5": ">=9.0.5"
    }
  }
}
